{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetBucketLocation",
        "s3:GetBucketVersioning"
      ],
      "Resource": [
        "arn:aws:s3:::deepref-uploads-production",
        "arn:aws:s3:::deepref-uploads-staging",
        "arn:aws:s3:::deepref-uploads-dev"
      ]
    },
    {
      "Sid": "ManageObjects",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:PutObjectAcl",
        "s3:GetObjectAcl",
        "s3:RestoreObject"
      ],
      "Resource": [
        "arn:aws:s3:::deepref-uploads-production/*",
        "arn:aws:s3:::deepref-uploads-staging/*",
        "arn:aws:s3:::deepref-uploads-dev/*"
      ]
    },
    {
      "Sid": "GetObjectMetadata",
      "Effect": "Allow",
      "Action": [
        "s3:HeadObject",
        "s3:GetObjectMetadata",
        "s3:GetObjectTagging",
        "s3:PutObjectTagging"
      ],
      "Resource": [
        "arn:aws:s3:::deepref-uploads-production/*",
        "arn:aws:s3:::deepref-uploads-staging/*",
        "arn:aws:s3:::deepref-uploads-dev/*"
      ]
    },
    {
      "Sid": "ManageMultipartUploads",
      "Effect": "Allow",
      "Action": [
        "s3:AbortMultipartUpload",
        "s3:ListMultipartUploadParts"
      ],
      "Resource": [
        "arn:aws:s3:::deepref-uploads-production/*",
        "arn:aws:s3:::deepref-uploads-staging/*",
        "arn:aws:s3:::deepref-uploads-dev/*"
      ]
    }
  ]
}
