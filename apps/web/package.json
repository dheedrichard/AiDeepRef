{
  "name": "deepref-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:ssr": "ng build --configuration production",
    "build:prod:stats": "ng build --configuration=production --stats-json",
    "serve:ssr:dev": "ng serve --configuration development",
    "serve:ssr": "node dist/deepref-web/server/server.mjs",
    "prerender": "ng run deepref-web:prerender",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "analyze:bundle": "npm run build:prod && node ../../scripts/track-performance.js",
    "analyze:size": "npm run build:prod:stats && echo 'Bundle stats generated'"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.13",
    "@angular/cdk": "^20.2.13",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/material": "^20.2.13",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.13",
    "@angular/router": "^20.3.0",
    "@angular/service-worker": "^20.3.13",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/entity": "^20.1.0",
    "@ngrx/router-store": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngrx/store-devtools": "^20.1.0",
    "@sentry/angular": "^8.55.0",
    "primeicons": "^7.0.0",
    "primeng": "^20.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/pwa": "^20.3.11",
    "@angular/ssr": "^20.3.11",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.5",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.10.1",
    "angular-eslint": "20.6.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "express": "^5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.2",
    "typescript-eslint": "8.46.3",
    "webpack-bundle-analyzer": "^5.0.1"
  }
}
