# DeepRef Web Environment Configuration
# Copy this file to .env and update with your values

# Application
NODE_ENV=development
APP_NAME=DeepRef Web
APP_VERSION=1.0.0

# API Configuration
API_URL=http://localhost:3000
API_TIMEOUT=30000
API_RETRY_COUNT=3

# Authentication
AUTH_TOKEN_KEY=deepref_auth_token
AUTH_REFRESH_TOKEN_KEY=deepref_refresh_token
AUTH_REDIRECT_URL=/dashboard

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CHAT=true
ENABLE_FILE_UPLOAD=true
ENABLE_NOTIFICATIONS=true

# Analytics
GOOGLE_ANALYTICS_ID=
MIXPANEL_TOKEN=

# Error Tracking
SENTRY_DSN=

# UI Configuration
DEFAULT_THEME=light
ITEMS_PER_PAGE=20
MAX_UPLOAD_SIZE=10485760

# External Services
WEBSOCKET_URL=ws://localhost:3000
STRIPE_PUBLISHABLE_KEY=

# Development
ENABLE_DEV_TOOLS=true
ENABLE_LOGGING=true
LOG_LEVEL=debug
