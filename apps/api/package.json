{
  "name": "@deepref/api",
  "version": "1.0.0",
  "description": "DeepRef AI Reference Verification Platform - Backend API",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:migration": "jest --config ./test/jest-migration.json",
    "test:security": "jest --testPathPattern=security --coverage --coverageDirectory=coverage/security",
    "test:ai-security": "jest --testPathPattern=security/ai --coverage",
    "test:security:verbose": "jest --testPathPattern=security --verbose --coverage",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate -d ormconfig.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d ormconfig.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ormconfig.ts",
    "migration:show": "npm run typeorm -- migration:show -d ormconfig.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ormconfig.ts",
    "schema:sync": "npm run typeorm -- schema:sync -d ormconfig.ts",
    "seed:ai-prompts": "ts-node -r tsconfig-paths/register src/database/seeds/run-ai-prompts-seed.ts",
    "seed:ai-prompts:update": "ts-node -r tsconfig-paths/register src/database/seeds/run-ai-prompts-seed.ts --update"
  },
  "keywords": [
    "nestjs",
    "api",
    "reference-verification",
    "ai"
  ],
  "author": "DeepRef Team",
  "license": "MIT",
  "dependencies": {
    "@nestjs/bull": "^10.2.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@sentry/node": "^7.120.4",
    "@sentry/profiling-node": "^7.120.4",
    "@types/cookie-parser": "^1.4.10",
    "@types/express-session": "^1.18.2",
    "argon2": "^0.44.0",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.7",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "ioredis": "^5.4.1",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "speakeasy": "^2.0.0",
    "typeorm": "^0.3.27",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.11",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^10.4.20",
    "@types/bcrypt": "^6.0.0",
    "@types/bull": "^4.10.0",
    "@types/connect-redis": "^0.0.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.19.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "redis-memory-server": "^0.10.0",
    "rimraf": "^6.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
